{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "db:init": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed",
    "dev": "ts-node src/index.ts",
    "format": "prettier --write",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:seed": "ts-node prisma/seed.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:file": "jest --runTestsByPath"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.3",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.0",
    "prisma": "^6.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "@types/multer": "^1.4.12",
    "argon2": "^0.43.0",
